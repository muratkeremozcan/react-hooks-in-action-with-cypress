{
  "projectId": "nr3y7v",
  "$schema": "https://on.cypress.io/cypress.schema.json",
  "baseUrl": "http://localhost:3000",
  "component": {
    "testFiles": "**/**/*.cy.{js,ts,jsx,tsx}",
    "componentFolder": "src"
  },
  "retries": {
    "runMode": 2,
    "openMode": 0
  },
  "experimentalPolyfill": true,
  "env": {
    "WAIT_ON_TIMEOUT": 1000,
    "grepFilterSpecs": true,
    "grepOmitFiltered": true
  }
}
