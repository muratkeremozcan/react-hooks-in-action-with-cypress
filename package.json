{
  "name": "react-hooks-in-action",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.1.1",
    "@testing-library/user-event": "14.1.1",
    "dayjs": "1.11.1",
    "history": "5.2.0",
    "launchdarkly-react-client-sdk": "2.25.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-query": "cd 3.34.16",
    "react-router-dom": "6.2.2",
    "react-scripts": "4.0.3",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@cypress/instrument-cra": "1.4.0",
    "@cypress/react": "5.12.4",
    "@cypress/webpack-dev-server": "1.8.4",
    "@faker-js/faker": "6.1.2",
    "@percy/cli": "1.0.8",
    "@percy/cypress": "3.1.1",
    "concurrently": "7.1.0",
    "cross-env": "7.0.3",
    "cy-spok": "1.5.2",
    "cypress": "9.4.1",
    "cypress-grep": "2.13.1",
    "cypress-ld-control": "1.1.3",
    "cypress-localstorage-commands": "1.7.0",
    "cypress-react-app-actions": "1.0.2",
    "cypress-real-events": "1.7.0",
    "cypress-should-really": "1.5.0",
    "dotenv": "16.0.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "4.3.8",
    "istanbul-lib-coverage": "3.2.0",
    "json-server": "0.17.0",
    "nyc": "15.1.0",
    "prettier": "2.6.2",
    "react-error-boundary": "3.1.4",
    "start-server-and-test": "1.14.0"
  },
  "scripts": {
    "dev": "concurrently -k yarn:start yarn:start:api",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:api": "json-server --watch db.json --port 3001 --delay 1000",
    "build": "react-scripts build",
    "test": "cross-env CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --ext=.js,.ts,.tsx",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:open-ct": "cypress open-ct",
    "cy:run-ct": "cypress run-ct --browser chrome",
    "cy:open-e2e": "concurrently yarn:start:api 'server-test 3000 cy:open'",
    "cy:run-e2e": "concurrently yarn:start:api 'server-test 3000 cy:run'",
    "coverage:reset": "rm -rf .nyc_output && rm -rf coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-expressions": "off",
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn cy:run-ct"
    }
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "/src/examples/**/"
    ]
  }
}
