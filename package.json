{
  "name": "react-hooks-in-action",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dayjs": "1.11.3",
    "history": "5.3.0",
    "launchdarkly-react-client-sdk": "2.26.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.4.0",
    "react-query": "3.39.1",
    "react-router-dom": "6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@applitools/eyes-cypress": "3.26.3",
    "@cypress/code-coverage": "3.10.0",
    "@cypress/instrument-cra": "1.4.0",
    "@faker-js/faker": "7.3.0",
    "@percy/cli": "1.5.1",
    "@percy/cypress": "3.1.2",
    "concurrently": "7.2.2",
    "cross-env": "7.0.3",
    "cy-spok": "1.5.2",
    "cypress": "10.3.0",
    "cypress-grep": "3.0.1",
    "cypress-ld-control": "1.1.3",
    "cypress-localstorage-commands": "2.1.0",
    "cypress-react-app-actions": "1.0.2",
    "cypress-real-events": "1.7.1",
    "cypress-should-really": "1.6.0",
    "dotenv": "16.0.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "istanbul-lib-coverage": "3.2.0",
    "json-server": "0.17.0",
    "nyc": "15.1.0",
    "prettier": "2.7.1",
    "react-error-boundary": "3.1.4",
    "start-server-and-test": "1.14.0"
  },
  "scripts": {
    "dev": "concurrently -k yarn:start yarn:start:api",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start:api": "json-server --watch db.json --port 3001 --delay 1000",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint --ext=.js,.ts,.tsx",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run --e2e --browser chrome",
    "cy:open-ct": "cypress open --component --browser chrome",
    "cy:run-ct": "cypress run --component --browser chrome",
    "cy:open-e2e": "concurrently yarn:start:api 'server-test 3000 cy:open'",
    "cy:run-e2e": "concurrently yarn:start:api 'server-test 3000 cy:run'",
    "coverage:reset": "rm -rf .nyc_output && rm -rf coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-expressions": "off",
      "no-undef": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn cy:run-ct"
    }
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "/src/examples/**/"
    ]
  }
}
